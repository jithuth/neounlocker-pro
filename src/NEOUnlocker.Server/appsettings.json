{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FirmwareSettings": {
    "StoragePath": "./FirmwareStorage",
    "MasterEncryptionKeyBase64": ""  // DEVELOPMENT ONLY: Leave empty to generate random key. MUST be set for production!
  },
  "SessionSettings": {
    "SessionExpiryMinutes": 15,
    "MaxConcurrentSessions": 100
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  }
}
