{
  "SerialPort": {
    "BaudRate": 115200,
    "DataBits": 8,
    "Parity": "None",
    "StopBits": "One",
    "ReadTimeout": 5000,
    "WriteTimeout": 5000
  },
  "Fastboot": {
    "ExecutablePath": "./Tools/fastboot.exe",
    "DetectionTimeoutSeconds": 60,
    "FlashTimeoutSeconds": 300
  },
  "Bootloaders": {
    "Directory": "./Resources/Bootloaders",
    "ModelMapping": {
      "E5573": "E5573.bin",
      "E8372": "E8372.bin",
      "E5577": "E5577.bin",
      "E3372": "E3372.bin"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    },
    "LogToFile": true,
    "LogFilePath": "./Logs/neounlocker.log"
  }
}
